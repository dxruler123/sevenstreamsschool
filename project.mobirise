ardAmount\" min=\"1\" max=\"3\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Image\" name=\"showImage\" checked>\n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" checked>\n        <input type=\"checkbox\" title=\"List\" name=\"showList\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Shadow\" name=\"hasShadow\" checked>\n        <input type=\"color\" title=\"Color\" name=\"cardColor\" value=\"#ffffff\" selected>\n        <header condition=\"showImage\">Image</header>\n        <select title=\"Align\" name=\"imageAlign\" condition=\"showImage\">\n            <option value=\"justify-content-start\" selected>Left</option>\n            <option value=\"justify-content-center\">Center</option>\n            <option value=\"justify-content-end\">Right</option>\n        </select>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"../_images/bg/3-bg.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#008d43\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"../_images/bg/1-bg.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"wrapper\">\n            <div class=\"row title-section {{titleAlign}}\" mbr-if=\"showTitle || showSubtitle\">\n                <div class=\"col-{{titleWidth}}\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style mb-4\" mbr-theme-style=\"display-2\" data-app-selector=\".title-section .mbr-section-title\" data-app-placeholder=\"Type Text\" mbr-if=\"showTitle\">\n                        <b>Our preschool dedicated classes</b>\n                    </h2>\n                    <h3 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".title-section .mbr-section-subtitle\" data-app-placeholder=\"Type Text\" mbr-if=\"showSubtitle\">\n                        Voluptas tempore perspiciatis distinctio eos, officia laudantium!\n                    </h3>\n                </div>\n            </div>\n            <div class=\"row {{contentAlign}}\">\n                <div class=\"col-12 col-lg-4\">\n                    <div class=\"card\">\n                        <div class=\"card-body\">\n                            <div class=\"mb-3 {{imageAlign}}\" mbr-class=\"{'d-flex': showImage}\" mbr-if=\"showImage\">\n                                <img src=\"../_images/vector/icons/1-icons.svg\" alt=\"Mobirise\" class=\"card-img-top\">\n                            </div>\n                            <h4 class=\"card-title mbr-section-subtitle mbr-fonts-style mb-4\" mbr-theme-style=\"display-5\" data-app-selector=\".mbr-section-subtitle\" data-app-placeholder=\"Type Text\" mbr-if=\"showCardTitle\">\n                                <b>Reporting on Individual Achievement</b>\n                            </h4>\n                            <ul cl